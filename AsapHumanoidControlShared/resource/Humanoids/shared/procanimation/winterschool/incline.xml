<ProcAnimation prefDuration="1.0" minDuration="1.0" maxDuration="1.0">
	<Rotation rotation="if(t &lt; 0.1,0,0.5 * a * sin((t-0.1) / 0.9 * pi * r)); lr * a * 0.5 * sin( t * pi * r);if (t &lt; 0.1,0, 0.25 * -a * 0.25 * sin((t-0.1) / 0.9 * pi * r))" target="vc4"/>
	<Rotation rotation="if(t &lt; 0.1,0,0.5 * a * sin((t-0.1) / 0.9 * pi * r)); lr * a * 0.5 * sin( t * pi * r);if (t &lt; 0.1,0, 0.25 * -a * 0.25 * sin((t-0.1) / 0.9 * pi * r))" target="skullbase"/>	
  <Parameter sid="a" description="magnitude" value="0.05"/>
  <Parameter sid="r" description="repeats" value="1"/>
  <Parameter sid="lr" description="lr" value="0.1"/>
  <KeyPosition id="ready" time="0.3" weight="1.0"/>
  <KeyPosition id="relax" time="0.7" weight="1.0"/>  
  <KeyPosition id="stroke" time="0.5" weight="1.0"/>
</ProcAnimation>

